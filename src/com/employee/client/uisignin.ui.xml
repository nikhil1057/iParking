<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.panel {
		background:
			url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRcHzbWDfAIeMwJeoQUHGMe7OqUmk2erm6U6W0T9sIm9XGQuDG5hA");
		background-size: 100%;
		padding: 0px !important;
		padding-top: 40% !important;
		padding-le "src/com/webpearl/client/signin.ui.xml"ft: 30%;
		padding-rigth: 60%;
	}
	
	.fieldPanel {
		background: white;
		padding: 6%;
	}
	
	.imgProfile {
		margin-top: -120px;
		width: 40%;
		margin-left: 30%;
		border: 5px solid #fff;
	}
	
	.rowAction {
		margin-top: 20px;
	}
	
	.login_demo {
		padding: 5%;
	}
	</ui:style>
	<g:HTMLPanel>
		<m:MaterialHeader marginTop="0" paddingTop="0">
			<m:MaterialNavBar backgroundColor="BLUE_DARKEN_4" marginTop="1"
				paddingTop="1">
				<m:MaterialNavBrand float="RIGHT" paddingLeft="50"
					text="Sign In" fontSize="2.3em" />
				<m:MaterialLink ui:field="link1" iconType="ARROW_BACK"
					textColor="WHITE" />
			</m:MaterialNavBar>
		</m:MaterialHeader>

		<m:MaterialRow ui:field="sip" addStyleNames="{style.login_demo}">
			<m:MaterialColumn grid="l4" offset="l4">
				<m:MaterialPanel shadow="1" addStyleNames="{style.panel}">
					<m:MaterialPanel addStyleNames="{style.fieldPanel}">
						<m:MaterialImage
							url="data:image/jpeg;base64,"
							type="CIRCLE" addStyleNames="{style.imgProfile} z-depth-1" />
						<m:MaterialTextBox ui:field="t1" type="EMAIL"
							label="Email" />
						<m:MaterialTextBox ui:field="t2" type="PASSWORD"
							label="Password" length="8" />
						<m:MaterialButton ui:field="btn" waves="LIGHT"
							text="Log In" width="100%" />
						<m:MaterialRow addStyleNames="{style.rowAction}">
							<m:MaterialColumn grid="s12 m12 l6">
								<m:MaterialCheckBox text="Keep Me Logged In" />
							</m:MaterialColumn>
						</m:MaterialRow>
					</m:MaterialPanel>
				</m:MaterialPanel>
			</m:MaterialColumn>
		</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 