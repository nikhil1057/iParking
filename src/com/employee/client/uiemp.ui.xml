<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
.panel{ background: url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRcHzbWDfAIeMwJeoQUHGMe7OqUmk2erm6U6W0T9sIm9XGQuDG5hA"); background-size: 100%; padding: 0px!important; padding-top:20% !important; padding-left: 30%; padding-rigth: 30%; } 
.fieldPanel{ background:white; padding:4%; } 
.imgProfile{ margin-top:-120px; width:40%; margin-left:30%; border:5px solid #fff; }
 .rowAction{ margin-top:20px; } 
.login_demo{ padding:1%; }
	}
	</ui:style>
	<g:HTMLPanel>
<m:MaterialHeader paddingTop="0" marginTop="0">
<m:MaterialNavBar paddingTop="1" marginTop="1" backgroundColor="BLUE_DARKEN_4">
<m:MaterialNavBrand fontSize="3.0em" text="Enter Tokens" paddingLeft="50" float="RIGHT"/>
<m:MaterialLink textColor="WHITE" iconType="ARROW_BACK" ui:field="link1"/>
</m:MaterialNavBar>
</m:MaterialHeader>
<m:MaterialRow ui:field="sip" addStyleNames="{style.login_demo}">
<m:MaterialColumn offset="l3" grid="s12 m10 l6">
<m:MaterialPanel marginTop="90" marginLeft="50" shadow="4">
<m:MaterialPanel addStyleNames="{style.fieldPanel}">
<m:MaterialTitle title="Token Numbers" textColor="BLUE" fontWeight="BOLD" textAlign="CENTER"/>
<m:MaterialTextBox ui:field="t1" allowBlank="false" label="ID" length="15"/>
<m:MaterialTextBox ui:field="t2" allowBlank="false" label="Tokens" length="3"/>
<m:MaterialButton backgroundColor="BLUE_DARKEN_4" text="SUBMIT" ui:field="valbtn" margin="8" waves="DEFAULT"/>

</m:MaterialPanel>
</m:MaterialPanel>
</m:MaterialColumn>
</m:MaterialRow>
</g:HTMLPanel>
</ui:UiBinder> 